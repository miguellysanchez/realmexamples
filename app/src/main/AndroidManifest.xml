<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.realmexample">

    <application
        android:name=".RealmExampleApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".part1.Part1Activity" />
        <activity android:name=".part2.Part2Activity" />
        <activity android:name=".part3.Part3Activity" />
        <activity android:name=".part4.Part4Activity" />
        <activity android:name=".part5.Part5SetupActivity"/>
        <activity android:name=".part5.Part5Activity" />
    </application>

</manifest>